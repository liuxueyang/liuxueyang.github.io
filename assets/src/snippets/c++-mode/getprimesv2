# -*- mode: snippet -*-
# name: getprimesv2
# key: getprimesv2
# --
const int N = 100010;
int cnt, primes[N];
bool st[N];

void get_primes_v2(int n) {
  cnt = 0;
  st[0] = st[1] = true;
  for (int i = 2; i <= n; ++i) {
    if (!st[i]) primes[cnt++] = i;
    for (int j = 0; primes[j] <= n / i; ++j) {
      st[primes[j] * i] = true;
      if (i % primes[j] == 0) break;
    }
  }
}

