/div><div class="line">                    <span class="keyword">else</span> <span class="keyword">if</span> (cc==<span class="string">'M'</span>) U*=<span class="number">1000000</span>;</div><div class="line">                    <span class="keyword">else</span> ;</div><div class="line">                &#125;</div><div class="line">                <span class="keyword">else</span> <span class="keyword">if</span> (ch==<span class="string">'P'</span>)</div><div class="line">                &#123;</div><div class="line">                    <span class="built_in">cin</span> &gt;&gt; P;</div><div class="line">                    cc = getchar();</div><div class="line">                    <span class="keyword">if</span> (cc==<span class="string">'m'</span>) P*=<span class="number">0.001</span>;</div><div class="line">                    <span class="keyword">else</span> <span class="keyword">if</span> (cc==<span class="string">'k'</span>) P*=<span class="number">1000</span>;</div><div class="line">                    <span class="keyword">else</span> <span class="keyword">if</span> (cc==<span class="string">'M'</span>) P*=<span class="number">1000000</span>;</div><div class="line">                    <span class="keyword">else</span> ;</div><div class="line">                &#125;</div><div class="line">                <span class="keyword">break</span>;</div><div class="line">            &#125;</div><div class="line">            ch = c;</div><div class="line">        &#125;</div><div class="line">        gets(s);</div><div class="line">        <span class="built_in">cout</span> &lt;&lt; setprecision(<span class="number">2</span>)&lt;&lt; fixed;</div><div class="line">        <span class="keyword">if</span> (P!=<span class="number">0</span>&amp;&amp;U!=<span class="number">0</span>)    <span class="built_in">cout</span>&lt;&lt;<span class="string">"I="</span>&lt;&lt;P/U&lt;&lt;<span class="string">'A'</span>&lt;&lt;<span class="built_in">endl</span>;</div><div class="line">        <span class="keyword">else</span> <span class="keyword">if</span> (P!=<span class="number">0</span>&amp;&amp;I!=<span class="number">0</span>)    <span class="built_in">cout</span>&lt;&lt;<span class="string">"U="</span>&lt;&lt;P/I&lt;&lt;<span class="string">'V'</span>&lt;&lt;<span class="built_in">endl</span>;</div><div class="line">        <span class="keyword">else</span> <span class="keyword">if</span> (I!=<span class="number">0</span>&amp;&amp;U!=<span class="number">0</span>)    <span class="built_in">cout</span>&lt;&lt;<span class="string">"P="</span>&lt;&lt;I*U&lt;&lt;<span class="string">'W'</span>&lt;&lt;<span class="built_in">endl</span>;</div><div class="line">        <span class="built_in">cout</span> &lt;&lt; <span class="built_in">endl</span>;</div><div class="line">    &#125;</div><div class="line"></div><div class="line">    <span class="keyword">return</span> <span class="number">0</span>;</div><div class="line">&#125;</div></pre></td></tr></table></figure>

      
